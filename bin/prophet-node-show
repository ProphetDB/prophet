#!/usr/bin/perl
use warnings;
use strict;

use Prophet::CLI;
my $cli = Prophet::CLI->new();
$cli->parse_record_cmd_args();
my $record = Prophet::Record->new(handle => $cli->handle, type => $cli->type);
$record->load(uuid => $cli->uuid);
print "id: ".$record->uuid."\n";
my $props = $record->get_props();
for (keys %$props) {
    print $_.": ".$props->{$_}."\n";
}



    
