#!/usr/bin/perl
use warnings;
use strict;

use Prophet::CLI;
my $cli = Prophet::CLI->new();
$cli->parse_record_cmd_args();

my $record = Prophet::Record->new(handle => $cli->handle, type => $cli->type);
my ($id, $results)= $record->create( props => $cli->args);
print "Created ". $cli->type." ".$record->uuid."\n";
